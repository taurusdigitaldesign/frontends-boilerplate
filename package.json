{
  "name": "greatfedjs",
  "version": "1.3.0",
  "description": "frontends boilerplate",
  "author": "GreatFED",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/pm_ziguang/frontends-boilerplate.git"
  },
  "scripts": {
    "clean:lib": "rimraf ./lib",
    "lib": "npm run clean:lib && webpack --config ./scripts/webpack/webpack.lib.js -p",

    "mock": "cross-env RUN_ENV=mock node ./scripts/server.js",
    "dev": "cross-env RUN_ENV=dev webpack-dev-server",
    "pro": "cross-env RUN_ENV=prod webpack-dev-server",

    "clean:dist": "rimraf ./dist",
    "build:dev": "npm run clean:dist && cross-env RUN_ENV=dev webpack",
    "build": "npm run clean:dist && cross-env RUN_ENV=prod webpack",
    "build:anal": "npm run clean:dist && webpack --anal=1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
