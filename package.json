    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/runtime-corejs3": "^7.11.2",
